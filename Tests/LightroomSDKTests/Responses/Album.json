{
  "base": "<base_url>/v2/catalogs/<catalog_id>/",
  "id": "<album_id>",
  "type": "album",
  "subtype": "<album_subtype>",
  "created": "<created_date>",
  "updated": "<updated_date>",
  "revision_ids": [
    "<album_revision_id1>",
    "<album_revision_id2>",
    "<album_revision_id3>"
  ],
  "payload": {
    "name": "<album_name>",
    "cover": {
      "id": "<cover_asset_id>"
    },
    "parent": {
      "id": "<parent_album_id>"
    }
  },
  "links": {
    "self": {
      "href": "albums/<album_id>"
    },
    "/rels/album_assets": {
